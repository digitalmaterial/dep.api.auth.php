{
  "name": "mtndep/api",
  "description": "Communicate with the MTN DEP API",
  "license": "MIT",
  "authors": [
    {
      "name": "Digital Material",
      "email": "dev@digitalmaterial.co.za"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "MTNDEP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GuzzleHttp\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.0",
    "aws/aws-sdk-php": "^3.19",
    "jakeasmith/http_build_url": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "ext-xdebug": "*",
    "phpunit/php-invoker": "^2.0"
  }
}
